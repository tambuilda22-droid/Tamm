--- [ â„ï¸ TAB HIá»†U á»¨NG CHILL - PHIÃŠN Báº¢N BÃƒO TUYáº¾T ] ---
local ChillTab = Window:CreateTab("â„ï¸ Hiá»‡u á»¨ng Chill", "snowflake")

ChillTab:CreateSection("CÃ i Ä‘áº·t BÃ£o Tuyáº¿t")

local snowEnabled = false
local snowflakes = {}
local snowFolder = nil
local maxSnowCount = 150 -- Máº·c Ä‘á»‹nh

-- HÃ m táº¡o háº¡t tuyáº¿t
local function createSnowflake()
    local p = Instance.new("Part")
    p.Size = Vector3.new(0.2, 0.2, 0.2)
    p.Material = Enum.Material.Neon
    p.Color = Color3.fromRGB(220, 240, 255)
    p.Transparency = 0.4
    p.CanCollide = false
    p.Anchored = true
    local mesh = Instance.new("SpecialMesh", p)
    mesh.MeshType = Enum.MeshType.Sphere
    return p
end

-- HÃ m cáº­p nháº­t sá»‘ lÆ°á»£ng tuyáº¿t
local function updateSnowCount(count)
    if not snowFolder then
        snowFolder = Instance.new("Folder", workspace)
        snowFolder.Name = "ChillSnowEffects"
    end
    
    -- XÃ³a bá»›t náº¿u giáº£m slider
    if #snowflakes > count then
        for i = #snowflakes, count + 1, -1 do
            if snowflakes[i] then
                snowflakes[i].part:Destroy()
                table.remove(snowflakes, i)
            end
        end
    -- ThÃªm má»›i náº¿u tÄƒng slider
    elseif #snowflakes < count then
        for i = #snowflakes + 1, count do
            table.insert(snowflakes, {
                part = createSnowflake(),
                offset = Vector3.new(math.random(-60, 60), math.random(0, 50), math.random(-60, 60)),
                speed = math.random(80, 150) / 100
            })
        end
    end
    
    -- Cáº­p nháº­t Parent dá»±a trÃªn tráº¡ng thÃ¡i Toggle
    for _, data in ipairs(snowflakes) do
        data.part.Parent = snowEnabled and snowFolder or nil
    end
end

-- Toggle Báº­t/Táº¯t
ChillTab:CreateToggle({
   Name = "ğŸŒ¨ï¸ KÃ­ch Hoáº¡t Tuyáº¿t RÆ¡i",
   CurrentValue = false,
   Callback = function(Value)
      snowEnabled = Value
      updateSnowCount(maxSnowCount)
   end,
})

-- Slider chá»‰nh Ä‘á»™ dÃ y (NHIá»€U)
ChillTab:CreateSlider({
   Name = "â„ï¸ Äá»™ DÃ y Cá»§a Tuyáº¿t (BÃ£o Tuyáº¿t)",
   Range = {32, 200},
   Increment = 10,
   CurrentValue = 150,
   Callback = function(Value)
      maxSnowCount = Value
      if snowEnabled then
          updateSnowCount(maxSnowCount)
      end
   end,
})

-- NÃºt Shader Láº¡nh (PhiÃªn báº£n Tá»‘i Vá»«a Vá»«a)
ChillTab:CreateButton({
   Name = "ğŸ¥¶ Shader ÄÃªm ÄÃ´ng (Tá»‘i Vá»«a Vá»«a)",
   Callback = function()
      local Lighting = game:GetService("Lighting")
      
      -- Dá»n dáº¹p cÃ¡c hiá»‡u á»©ng cÅ© Ä‘á»ƒ trÃ¡nh bá»‹ chá»“ng chÃ©o (Lags/Lá»—i mÃ u)
      for _, v in pairs(Lighting:GetChildren()) do
          if v:IsA("ColorCorrectionEffect") or v:IsA("BloomEffect") or v:IsA("Atmosphere") then 
              v:Destroy() 
          end
      end

      -- 1. Chá»‰nh mÃ u sáº¯c tá»•ng thá»ƒ (Color Correction)
      local color = Instance.new("ColorCorrectionEffect", Lighting)
      color.Saturation = -0.15 -- Giáº£m Ä‘á»™ tÆ°Æ¡i vá»«a pháº£i (khÃ´ng quÃ¡ xÃ¡m)
      color.Contrast = 0.1     -- TÄƒng tÆ°Æ¡ng pháº£n nháº¹ Ä‘á»ƒ sáº¯c nÃ©t
      color.TintColor = Color3.fromRGB(190, 215, 255) -- MÃ u xanh nháº¡t hÆ¡n, Ä‘á»¡ bá»‹ Ã¡m xanh náº·ng

      -- 2. Hiá»‡u á»©ng sÆ°Æ¡ng mÃ¹/khÃ­ quyá»ƒn (Atmosphere)
      local atm = Instance.new("Atmosphere", Lighting)
      atm.Density = 0.35      -- Giáº£m Ä‘á»™ dÃ y sÆ°Æ¡ng mÃ¹ Ä‘á»ƒ nhÃ¬n rÃµ hÆ¡n (Vá»«a pháº£i)
      atm.Color = Color3.fromRGB(150, 180, 220)
      atm.Glare = 0.2         -- ThÃªm chÃºt Ã¡nh sÃ¡ng pháº£n chiáº¿u nháº¹
      
      -- 3. Äiá»u chá»‰nh Ä‘á»™ sÃ¡ng há»‡ thá»‘ng
      Lighting.Brightness = 1.0           -- Giá»¯ Ä‘á»™ sÃ¡ng gá»‘c nhÆ°ng sáº½ tá»‘i Ä‘i nhá» Ambient
      Lighting.OutdoorAmbient = Color3.fromRGB(60, 75, 100) -- MÃ u bÃ³ng tá»‘i xanh dá»‹u, khÃ´ng Ä‘en ká»‹t
      Lighting.ClockTime = 18             -- Chá»‰nh vá» lÃºc hoÃ ng hÃ´n/cháº­p tá»‘i Ä‘á»ƒ cÃ³ vibe tá»‘t nháº¥t
      Lighting.ExposureCompensation = -0.5 -- Háº¡ sÃ¡ng má»™t chÃºt Ä‘á»ƒ cÃ³ cáº£m giÃ¡c Ä‘Ãªm láº¡nh

      Rayfield:Notify({
         Title = "Shader", 
         Content = "ÄÃ£ báº­t vibe ÄÃªm ÄÃ´ng dá»‹u mÃ¡t!", 
         Duration = 3
      })
   end,
})

--- [ âš¡Há»† THá»NG VÃ’NG Láº¶P CORE ] ---

-- God Mode Loop
RunService.Heartbeat:Connect(function()
    if _G.GodMode and lp.Character and lp.Character:FindFirstChild("Humanoid") then
        lp.Character.Humanoid.Health = lp.Character.Humanoid.MaxHealth
    end
end)

-- Farm Loop (âš¡Báº¢N NÃ‚NG Cáº¤P: DÃNH Äáº N + DAMAGE CAO)
task.spawn(function()
    while task.wait() do -- Sá»­ dá»¥ng task.wait() máº·c Ä‘á»‹nh Ä‘á»ƒ mÆ°á»£t hÆ¡n
        if _G.AutoFarmCoin or _G.AutoKillLan then
            local char = lp.Character
            local head = char and char:FindFirstChild("Head")
            local enemies = workspace:FindFirstChild("Zombies") or workspace:FindFirstChild("Enemies")
            
            if head and enemies then
                local target = nil
                local dist = settings.range

                -- QuÃ©t tÃ¬m má»¥c tiÃªu gáº§n nháº¥t
                for _, z in ipairs(enemies:GetChildren()) do
                    local zRoot = z:FindFirstChild("HumanoidRootPart")
                    local zHum = z:FindFirstChild("Humanoid")
                    
                    if zRoot and zHum and zHum.Health > 0 then
                        local magnitude = (zRoot.Position - head.Position).Magnitude
                        if magnitude < dist then
                            dist = magnitude
                            target = z
                        end
                    end
                end

                -- Náº¿u tÃ¬m tháº¥y má»¥c tiÃªu
                if target then
                    local zRoot = target:FindFirstChild("HumanoidRootPart")
                    local weapon = _G.AutoKillLan and "PzF 3" or "SMAW"
                    
                    -- Ã‰P Tá»ŒA Äá»˜ DÃNH VÃ€O Má»¤C TIÃŠU (Sá»­a lá»—i bay xuyÃªn)
                    local args = {
                        [1] = {
                            ["ModelType"] = "Character",
                            ["TargetPos"] = zRoot.Position, -- LuÃ´n báº¯n vÃ o tÃ¢m quÃ¡i
                            ["Model"] = target,
                            ["PlayerName"] = lp.Name,
                            ["WeaponName"] = weapon,
                            ["SourcePart"] = head,
                            ["SourcePos"] = head.Position
                        }
                    }
                    
                    -- Gá»¬I PACKET THEO Tá»C Äá»˜ Cá»¦A SLIDER
                    -- settings.damage_mult chÃ­nh lÃ  sá»‘ láº§n nhÃ¢n sÃ¡t thÆ°Æ¡ng
                    for i = 1, (settings.damage_mult or 10) do
                        remoteF:FireServer(unpack(args))
                    end
                    
                    -- Khoáº£ng nghá»‰ dá»±a trÃªn Slider Ä‘á»ƒ trÃ¡nh bá»‹ kick (Anti-cheat)
                    task.wait(settings.attack_speed or 0.1) 
                end
            end
        end
    end
end)

task.spawn(function()
    while true do 
        task.wait(settings.attack_speed or 0.1)
        if _G.AutoFarmCoin or _G.AutoKillLan then
            -- ... (Ä‘oáº¡n code báº¯n Ä‘áº¡n mÃ¬nh vá»«a tá»‘i Æ°u á»Ÿ trÃªn) ...
        end
    end
end)

local function SetupESP(zombie)
    -- Chá» 0.5 giÃ¢y Ä‘á»ƒ Zombie load Ä‘á»§ cÃ¡c bá»™ pháº­n tá»« Server
    task.wait(0.1)
    
    if _G.ESP_Zombie and zombie:IsA("Model") then
        -- Kiá»ƒm tra xem Ä‘Ã£ cÃ³ Highlight chÆ°a Ä‘á»ƒ trÃ¡nh trÃ¹ng láº·p
        if not zombie:FindFirstChild("Highlight") then
            local hl = Instance.new("Highlight")
            hl.Parent = zombie
            hl.FillColor = Color3.fromRGB(255, 0, 0)
            hl.OutlineColor = Color3.fromRGB(255, 255, 255)
            hl.FillTransparency = 0.5
        end
    end
end

-- Káº¿t ná»‘i vá»›i Folder chá»©a Zombie
local enemyFolder = workspace:FindFirstChild("Zombies") or workspace:FindFirstChild("Enemies")

if enemyFolder then
    -- Khi cÃ³ má»™t con Zombie má»›i xuáº¥t hiá»‡n trong Folder
    enemyFolder.ChildAdded:Connect(function(newChild)
        SetupESP(newChild)
    end)
end

-- â„VÃ²ng láº·p Render xá»­ lÃ½ di chuyá»ƒn tuyáº¿t
RunService.RenderStepped:Connect(function(dt)
    if snowEnabled and lp.Character and lp.Character:FindFirstChild("HumanoidRootPart") then
        if #snowflakes == 0 then
            snowFolder = snowFolder or Instance.new("Folder", workspace)
            for i = 1, 100 do
                local p = Instance.new("Part")
                p.Size = Vector3.new(0.2, 0.2, 0.2)
                p.Material = Enum.Material.Neon
                p.CanCollide = false
                p.Anchored = true
                p.Parent = snowFolder
                table.insert(snowflakes, {part = p, offset = Vector3.new(math.random(-50, 50), math.random(0, 40), math.random(-50, 50))})
            end
        end
        
        local rootPos = lp.Character.HumanoidRootPart.Position
        for _, data in ipairs(snowflakes) do
            data.offset = data.offset - Vector3.new(0, 15 * dt, 0)
            if data.offset.Y < -10 then
                data.offset = Vector3.new(math.random(-50, 50), 30, math.random(-50, 50))
            end
            data.part.Position = rootPos + data.offset
        end
    end
end)

