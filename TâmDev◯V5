TrollTab:CreateSlider({
    Name = "ğŸ”„ Äá»™ nghiÃªng/Xoay thÃ¢n hÃ¬nh",
    Range = {0, 640},
    Increment = 1,
    CurrentValue = 0,
    Callback = function(v)
        local char = lp.Character
        if not char then return end

        local rad = math.rad(v)
        -- C0 chuáº©n Ä‘á»ƒ xoay quanh trá»¥c Ä‘á»©ng mÃ  khÃ´ng lÃ m biáº¿n dáº¡ng tÆ° tháº¿ gá»‘c quÃ¡ má»©c
        local rotationCFrame = CFrame.Angles(0, 0, rad)

        if char:FindFirstChild("LowerTorso") and char.LowerTorso:FindFirstChild("Root") then
            -- R15: RootJoint thÆ°á»ng náº±m trong LowerTorso
            char.LowerTorso.Root.C0 = CFrame.new(0, 0, 0) * rotationCFrame
        elseif char:FindFirstChild("HumanoidRootPart") and char.HumanoidRootPart:FindFirstChild("RootJoint") then
            -- R6: RootJoint náº±m trong HumanoidRootPart
            char.HumanoidRootPart.RootJoint.C0 = CFrame.new(0, 0, 0) * rotationCFrame
        end
    end,
})

-- NÃºt Reset nhanh tÆ° tháº¿ vá» máº·c Ä‘á»‹nh
TrollTab:CreateButton({
    Name = "ğŸ”„ Reset TÆ° Tháº¿ (Tháº³ng Ä‘á»©ng)",
    Callback = function()
        local char = lp.Character
        if not char or not char:FindFirstChild("Humanoid") then return end

        -- GiÃ¡ trá»‹ máº·c Ä‘á»‹nh tuyá»‡t Ä‘á»‘i cá»§a Roblox (khÃ´ng Ã©p gÃ³c xoay 180 hay -90)
        local originC0 = CFrame.new(0, 0, 0) * CFrame.Angles(math.rad(-90), math.rad(180), 0)

        if char:FindFirstChild("LowerTorso") and char.LowerTorso:FindFirstChild("Root") then
            char.LowerTorso.Root.C0 = originC0
        end
        
        if char:FindFirstChild("HumanoidRootPart") and char.HumanoidRootPart:FindFirstChild("RootJoint") then
            char.HumanoidRootPart.RootJoint.C0 = originC0
        end
        
        -- Reset Ä‘á»™ cao
        char.Humanoid.HipHeight = 2
        
        -- Máº¹o: Ã‰p nhÃ¢n váº­t nháº£y nháº¹ Ä‘á»ƒ Engine Roblox cáº­p nháº­t láº¡i CFrame ngay láº­p tá»©c
        char.Humanoid.Jump = true
        
        Rayfield:Notify({
            Title = "Há»‡ Thá»‘ng",
            Content = "ÄÃ£ reset tÆ° tháº¿ vá» máº·c Ä‘á»‹nh!",
            Duration = 2
        })
    end,
})

